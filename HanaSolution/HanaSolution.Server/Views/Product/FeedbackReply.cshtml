@model IEnumerable<HanaSolution.Data.ViewModels.ReplyView>

    <table class="table table-striped table-hover">
        @if (Model != null && Model.Count() > 0)
        {
            foreach (var item in Model)
            {
                <tr>

                    <td class="open-view"><span class="msgtext">@item.Comment</span></td>
                    <td class="open-view"><span class="msg_time">@String.Format("{0:dd/MM/yyyy hh:mm}",item.Date)</span></td>
                </tr>
            }
        }

    </table>
